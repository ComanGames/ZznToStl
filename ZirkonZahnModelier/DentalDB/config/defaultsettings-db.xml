  <!--dentalPrj, iniPrj -->
  <ProjectFormat>dentalPrj</ProjectFormat>
  <!-- to store patient name set to : 1, yes, on or true-->
  <StorePatientInfos>0</StorePatientInfos>
  <!-- 
  keys for FilenameTemplate and PathTemplate
                 %d: date
                 %n: practice number
                 %p: practice name
                 %f: patient first name
                 %l: patient last name
                 %c: country
                 %s: Schalennr
  	
  This default setting is recommended by Exocad:
  <PathTemplate>%d_%n-%s</PathTemplate>
  

    
  -->
  <PathTemplate>%p\%l,%f</PathTemplate>
  <FilenameTemplate>%d_%n-%s</FilenameTemplate>
