<!-- DO NOT MODIFY THIS FILE!
   
     It will be overwritten during update process
     
     Instead, copy the colors that you want to change to "Colors.xaml" and make your changes there
     (you can remove the tags that remain unchanged - entries in Colors.xaml
     will override entries in DefaultColors.xaml) -->

<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Default Colors -->
    <Color x:Key="BlackColor">#FF000000</Color>
    <Color x:Key="WhiteColor">#FFFFFFFF</Color>
	<Color x:Key="GroupSelectorForegroundColor">#FFFFFFFF</Color>
    <Color x:Key="FloatingWindowBorderColor">#FF473A6D</Color>
		
	<!-- Border is disabled by default, so the above color definition will not have an effect unless you also
	     change the border thickness below - set the four values Top, Left, Right, Bottom to "1" or "2" to add a border -->
		 
	<Thickness x:Key="FloatingWindowBorderThickness" Top="0" Left="0" Right="0" Bottom ="0" />
	
    <Color x:Key="Gray1">#FFCCCCCC</Color>
    <Color x:Key="Gray2">#FFfbfaff</Color>
    <Color x:Key="Gray3">#FF333333</Color>
    <Color x:Key="Gray4">#FFB9B9B9</Color>
    <Color x:Key="Gray5">#FFD8D8D9</Color>
    <Color x:Key="Gray6">#FF9D9D9D</Color>
    <Color x:Key="Gray7">#FFfbfaff</Color>				<!-- group box background color. slightly darker variant. -->
    <Color x:Key="Gray8">#FFE0E0E0</Color>
    <Color x:Key="Gray9">#FFA59F93</Color>
    <Color x:Key="Gray10">#FFEEEEEE</Color>
    <Color x:Key="Gray11">#7FA9A9A9</Color>
    <Color x:Key="Gray12">#A5F7F7F7</Color>
    <Color x:Key="Gray13">#5EC9C9C9</Color>
	<Color x:Key="Gray14">#FF7E7E7E</Color>
    <Color x:Key="TextBoxText">#FF414141</Color>
    <Color x:Key="NormalForegroundColor">#FF000000</Color>
    <Color x:Key="HoverForegroundColor">#FFFFFFFF</Color>
	
	<Color x:Key="TransparentWhiteColor">#00FFFFFF</Color>
    <Color x:Key="SemiTransparentWhiteColor">#55FFFFFF</Color>
    <Color x:Key="TransparentLightLightColor">#A5FFFFFF</Color>


    <!-- CORE CONTROL COLORS-->
    <Color x:Key="ValidationColor1">#052A2E31</Color>
    <Color x:Key="ValidationColor2">#152A2E31</Color>
    <Color x:Key="ValidationColor3">#252A2E31</Color>
    <Color x:Key="ValidationColor4">#352A2E31</Color>
    <Color x:Key="ValidationColor5">#FFDC000C</Color>
    <Color x:Key="ControlsValidationColor">#FFDB000C</Color>
    
	<!-- Brand Colors: This are the brand colors for the company -->
    
	<Color x:Key="BrandColor1">#473A6D</Color>    <!-- Should be dark color. E.g. exocad Purple -->
    <Color x:Key="BrandColor1MouseOver">#584e7b</Color>    <!-- slightly lighter variant -->
	
	<Color x:Key="ToolWindowTitleColor">#584e7b</Color>    <!-- tool window title. can be same as BrandColor1MouseOver -->
	
	<Color x:Key="ButtonColor">#473A6D</Color>    <!-- Should be dark color. Can be same as BrandColor1 -->    
	<Color x:Key="ButtonColorMouseOver">#584e7b</Color>    <!-- slightly lighter variant -->
	
	<Color x:Key="ButtonPressedColor">#000000</Color>
    <SolidColorBrush x:Key="ButtonPressedBackground" Color="{DynamicResource ButtonPressedColor}"/>
	
	
    <Color x:Key="BrandColor2">#EE895F</Color>    <!-- Should be dark and 'highlighty'. E.g. exocad Orange -->
    <Color x:Key="BrandColor2MouseOver">#ED652F</Color>    <!-- slightly lighter variant -->
	<Color x:Key="BrandColor2Border">#AB6964</Color>    <!-- darker variant -->
	
	<!-- important: Also set this color in the BorderBrush definitions in ToggleButtonStyle.xaml -
	  (replace #AB6964 by your desired color) - due to WPF bugs, it is not possible to define BorderBrush as 
	  a DynamicResource - see http://stackoverflow.com/questions/17789648/dynamicresource-color-doesnt-work-for-borderbrush-on-a-border-bug
	  for details -->
	
	<Color x:Key="ToolbarBackgroundColor">#000000</Color>    <!-- Tool bar on the right. Should be dark color, black recommended -->
	<Color x:Key="ToolWindowBackgroundColor">#ffffff</Color>    <!-- tool window background color. White or very light color -->
	<Color x:Key="MessageBoxBackgroundColor">#fbfaff</Color>    <!-- message box background color -->
	
	
	<Color x:Key="ExostyleProgressBarColor">#584e7b</Color>    <!-- background color for progress bar. -->
	
	<Color x:Key="ExpertToolbarBackgroundColor">#FFFFFF</Color>    <!-- background color for expert mode toolbar. Should be light -->
	<Color x:Key="ExpertModeToolbarMenu">#000000</Color>    <!-- foreground color for expert mode toolbar. Should have good contrast to ExpertToolbarBackgroundColor -->
	<Color x:Key="ExpertModeToolbarAdvancedMenu">#584e7b</Color>    <!-- foreground color for expert mode toolbar. Should have good contrast to ExpertToolbarBackgroundColor -->
	<Color x:Key="ExpertModeToolbarMenuHighlight">#ED652F</Color>    <!-- foreground color for expert mode toolbar.  Should have good contrast to ExpertToolbarBackgroundColor -->	
	
    <SolidColorBrush x:Key="BrandBrush1" Color="{DynamicResource BrandColor1}" />
    <SolidColorBrush x:Key="BrandBrush1MouseOver" Color="{DynamicResource BrandColor1MouseOver}" />
	<SolidColorBrush x:Key="ExpertModeToolbarAdvancedMenuBrush" Color="{DynamicResource ExpertModeToolbarAdvancedMenu}" />
	<SolidColorBrush x:Key="ExpertModeToolbarMenuBrush" Color="{DynamicResource ExpertModeToolbarMenu}" />
	<SolidColorBrush x:Key="ExpertModeToolbarMenuHighlightBrush" Color="{DynamicResource ExpertModeToolbarMenuHighlight}" />
	<SolidColorBrush x:Key="ExpertModeBackgroundBrush" Color="{DynamicResource ExpertToolbarBackgroundColor}" />

	<SolidColorBrush x:Key="ButtonBrush" Color="{DynamicResource ButtonColor}" />
    <SolidColorBrush x:Key="ButtonBrushMouseOver" Color="{DynamicResource ButtonColorMouseOver}" />

	
    <SolidColorBrush x:Key="BrandBrush2" Color="{DynamicResource BrandColor2}" />
	<SolidColorBrush x:Key="BrandBrush2Border" Color="#AB6964" />
	<SolidColorBrush x:Key="BrandBrush3Border" Color="#EC865C" />


	
	<SolidColorBrush x:Key="ToolbarBackgroundBrush" Color="{DynamicResource ToolbarBackgroundColor}" />
	<SolidColorBrush x:Key="ExostyleProgressBarBrush" Color="{DynamicResource ExostyleProgressBarColor}" />

    <Color x:Key="ButtonBackgroundColor">#fbfaff</Color>    <!-- Should be light -->
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="{DynamicResource BrandColor1}" />
	
	<Color x:Key="GroupSelectorDefaultColor">#FF584e7b</Color>
	
    
    <!-- Background Colors for Windows (Used by MultiMessageBox / ToolWindows, etc.) -->
    <SolidColorBrush x:Key="Background1Brush" Color="{DynamicResource MessageBoxBackgroundColor}" />
    <SolidColorBrush x:Key="Background2Brush" Color="{DynamicResource MessageBoxOuterBackgroundColor}" />
    <SolidColorBrush x:Key="ToolWindowBackgroundBrush" Color="{DynamicResource ToolWindowBackgroundColor}" />
    <SolidColorBrush x:Key="ToolWindowTitleBrush" Color="{DynamicResource ToolWindowTitleColor}" />
	
	<!-- DentalDB window colors  -->
    <Color x:Key="Background1">#FFF4F4F4</Color> <!-- DentalDB left panel and some dialogs -->
    <Color x:Key="Background2">#FFFFFFFF</Color> <!-- DentalDB center panel -->
	<Color x:Key="Background3">#FFF4F4F4</Color> <!-- DentalDB right panel -->
  
	<!-- Alternating colors for zebra tables (e.g. 'Load')  -->
    <Color x:Key="ZebraTable1">#FFFF0000</Color> <!-- should be white or very light color -->
    <Color x:Key="ZebraTable2">#FFE0E0E0</Color> <!-- still light, but slightly darker -->

	    <!--BEGIN:Time Legend TreatmentListView -->
    <Color x:Key="Interval1">#D2F1E8</Color>
    <Color x:Key="Interval2">#79D6BB</Color>
    <Color x:Key="Interval3">#1FBA8E</Color>
    <Color x:Key="Interval4">#00916a</Color>

  
    <!-- *************BRUSHES START************* -->
    
    <!-- Foreground: This is used for text on Button, ComboBox, GroupBox, etc. -->
    <SolidColorBrush x:Key="ForegroundBrush" Color="#000000"/>
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#888888"/>
    
    <!-- Background -->
    <SolidColorBrush x:Key="BackgroundBrush" Color="{DynamicResource Gray1}"/>
    <SolidColorBrush x:Key="LightBackgroundBrush" Color="{DynamicResource Gray7}"/>
    <SolidColorBrush x:Key="SemiLightBackgroundBrush" Color="#EAEAEA"/>
    <SolidColorBrush x:Key="DarkBackgroundBrush" Color="{DynamicResource Gray2}"/>
    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="{DynamicResource Gray10}"/>
    <SolidColorBrush x:Key="PressedBackgroundBrush" Color="{DynamicResource Gray8}"/>      
    
    <!-- UNIVERSAL CONTROL BRUSHES -->
    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="{DynamicResource WhiteColor}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{DynamicResource WhiteColor}"/>
	<SolidColorBrush x:Key="GroupSelectorForegroundBrush" Color="{DynamicResource GroupSelectorForegroundColor}"/>
    <SolidColorBrush x:Key="FloatingWindowBorderBrush" Color="{DynamicResource FloatingWindowBorderColor}"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{DynamicResource BlackColor}"/>
    <SolidColorBrush x:Key="TextBrush" Color="{DynamicResource BlackColor}" />    
    <SolidColorBrush x:Key="LabelTextBrush" Color="{DynamicResource BlackColor}" />
    <SolidColorBrush x:Key="WhiteColorBrush" Color="{DynamicResource WhiteColor}" />
    <SolidColorBrush x:Key="BlackColorBrush" Color="{DynamicResource BlackColor}" />

    <SolidColorBrush x:Key="NavigationBorderBrush" Color="#FF9D9492" />
    <SolidColorBrush x:Key="NavigationForegroundBrush" Color="{DynamicResource BlackColor}"/>
    <SolidColorBrush x:Key="PageBorderBrush" Color="#FFB2B2B2" />
    <SolidColorBrush x:Key="BodyTextColorBrush" Color="#FF313131"/>
    <SolidColorBrush x:Key="ControlsDisabledBrush" Color="{DynamicResource TransparentLightLightColor}"/>
    <SolidColorBrush x:Key="DisabledWhiteColorBrush" Color="{DynamicResource WhiteColor}" />
    <SolidColorBrush x:Key="TransparentWhiteBrush" Color="{DynamicResource TransparentWhiteColor}"/>
    <SolidColorBrush x:Key="SemiTransparentWhiteBrush" Color="#55FFFFFF"/>

    <SolidColorBrush x:Key="GrayBrush1" Color="{DynamicResource Gray1}"/>
    <SolidColorBrush x:Key="GrayBrush2" Color="{DynamicResource Gray2}"/>
    <SolidColorBrush x:Key="GrayBrush3" Color="{DynamicResource Gray3}"/>
    <SolidColorBrush x:Key="GrayBrush4" Color="{DynamicResource Gray4}"/>
    <SolidColorBrush x:Key="GrayBrush5" Color="{DynamicResource Gray5}"/>
    <SolidColorBrush x:Key="GrayBrush6" Color="{DynamicResource Gray6}"/>
    <SolidColorBrush x:Key="GrayBrush7" Color="{DynamicResource Gray7}"/>
    <SolidColorBrush x:Key="GrayBrush8" Color="{DynamicResource Gray8}"/>
    <SolidColorBrush x:Key="GrayBrush9" Color="{DynamicResource Gray9}"/>
    <SolidColorBrush x:Key="GrayBrush10" Color="{DynamicResource Gray10}"/>
    <SolidColorBrush x:Key="GrayBrush11" Color="{DynamicResource Gray11}"/>
    <SolidColorBrush x:Key="GrayBrush12" Color="{DynamicResource Gray12}"/>
    <SolidColorBrush x:Key="GrayBrush13" Color="{DynamicResource Gray13}"/>
	<SolidColorBrush x:Key="GrayBrush14" Color="{DynamicResource Gray14}"/>

    <SolidColorBrush x:Key="HoverHyperlinkForegroundBrush" Color="{DynamicResource BlackColor}"/>
    <SolidColorBrush x:Key="HoverHyperlinkBackgroundBrush" Color="{DynamicResource WhiteColor}"/>

    <SolidColorBrush x:Key="TextBoxBorderBrush" Color="{DynamicResource Gray1}" />
    <SolidColorBrush x:Key="ControlBorderBrush" Color="{DynamicResource Gray1}" />
    <SolidColorBrush x:Key="TextBoxMouseOverBorderBrush" Color="{DynamicResource BlackColor}" />
    <SolidColorBrush x:Key="TextBoxMouseOverInnerBorderBrush" Color="{DynamicResource BlackColor}" />

    <SolidColorBrush x:Key="CheckBoxBrush" Color="{DynamicResource Gray2}" />
    <SolidColorBrush x:Key="CheckBoxMouseOverBrush" Color="{DynamicResource Gray3}" />
    <LinearGradientBrush x:Key="CheckBoxBackgroundBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{DynamicResource Gray5}" Offset="0"/>
        <GradientStop Color="{DynamicResource WhiteColor}" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="MenuItemDisabledBrush" Color="{DynamicResource Gray6}" />

    <SolidColorBrush x:Key="ThumbBrush" Color="{DynamicResource Gray4}" />

    <SolidColorBrush x:Key="ItemSelectedBrush" Color="{DynamicResource Gray5}" />

    <SolidColorBrush x:Key="SliderTrackBrush" Color="{DynamicResource Gray6}" />


    <SolidColorBrush x:Key="ComboBoxPopupBrush" Color="{DynamicResource Gray9}" />    

    <DropShadowEffect x:Key="DropShadowBrush" Direction="330" Opacity="0.3" ShadowDepth="0" BlurRadius="6"/>
    
    <SolidColorBrush x:Key="SeperatorBrush" Color="#FFC4C4C5"/>

    <!-- Controls -->
    <SolidColorBrush x:Key="BackgroundNormalBrush" Color="#CDCDCD"/>
    <SolidColorBrush x:Key="BorderNormalBrush" Color="#54545C"/>

    <SolidColorBrush x:Key="BackgroundHighlightedBrush" Color="#A6A6A6"/>
    <SolidColorBrush x:Key="BorderHighlightedBrush" Color="#6A6A75"/>

    <SolidColorBrush x:Key="BackgroundSelectedBrush" Color="#606060"/>
    <SolidColorBrush x:Key="BorderSelectedBrush" Color="#1C97EA"/>

    <SolidColorBrush x:Key="BackgroundDisabledBrush" Color="#BFBFBF"/>
    <SolidColorBrush x:Key="BorderDisabledBrush" Color="#FF00FF"/> <!-- TODO: Define -->

    <SolidColorBrush x:Key="ForegroundDisabledBrush" Color="#7F7F7F"/>
	
	<SolidColorBrush x:Key="ExpertToolbarBackgroundBrush" Color="{DynamicResource ExpertToolbarBackgroundColor }" />

    <!-- Scrollbar -->
    <SolidColorBrush x:Key="ScrollBarBackgroundBrush" Color="#F0F0F0" />
    <SolidColorBrush x:Key="ScrollBarDisabledBackgroundBrush" Color="#EDEDED" />

    <!-- LinkButton -->
    <SolidColorBrush x:Key="LinkButtonForeground" Color="{DynamicResource BrandColor2}"/>
    <SolidColorBrush x:Key="LinkButtonForegroundHighlighted" Color="{DynamicResource BrandColor2MouseOver}"/>


    <!-- CONTROL VALIDATION BRUSHES -->
    <SolidColorBrush x:Key="ControlsValidationBrush" Color="{DynamicResource ControlsValidationColor}"/>
    <SolidColorBrush x:Key="ValidationBrush1" Color="{DynamicResource ValidationColor1}"/>
    <SolidColorBrush x:Key="ValidationBrush2" Color="{DynamicResource ValidationColor2}"/>
    <SolidColorBrush x:Key="ValidationBrush3" Color="{DynamicResource ValidationColor3}"/>
    <SolidColorBrush x:Key="ValidationBrush4" Color="{DynamicResource ValidationColor4}"/>
    <SolidColorBrush x:Key="ValidationBrush5" Color="{DynamicResource ValidationColor5}"/>


    <LinearGradientBrush x:Key="ButtonNormalBackground" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#F3F3F3" Offset="0"/>
        <GradientStop Color="#EBEBEB" Offset="0.5"/>
        <GradientStop Color="#DDDDDD" Offset="0.5"/>
        <GradientStop Color="#CDCDCD" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ButtonNormalBorder" Color="#FF707070"/>
    
    <!-- WPF default colours -->
    <SolidColorBrush x:Key="{x:Static SystemColors.WindowBrushKey}" Color="{DynamicResource WhiteColor}" />
    <SolidColorBrush x:Key="{x:Static SystemColors.ControlTextBrushKey}" Color="{DynamicResource WhiteColor}" />
	
	<!-- DentalDB -->
	
		<!-- actual Background1 and Background 2 DentalDB colors defined in DefaultColors.xaml -->
	
    <SolidColorBrush x:Key="BackgroundDentalDBLeftRight" Color="{DynamicResource Background1}" /> <!-- dark brush -->
    <SolidColorBrush x:Key="BackgroundDentalDBCenter" Color="{DynamicResource Background2}" /> <!-- light brush -->

	
	
    <SolidColorBrush x:Key="Interval1Brush" Color="{DynamicResource Interval1}" />
    <SolidColorBrush x:Key="Interval2Brush" Color="{DynamicResource Interval2}" />
    <SolidColorBrush x:Key="Interval3Brush" Color="{DynamicResource Interval3}" />
    <SolidColorBrush x:Key="Interval4Brush" Color="{DynamicResource Interval4}" />
	
	<SolidColorBrush x:Key="ZebraTableBrush1" Color="{DynamicResource ZebraTable1}" />
    <SolidColorBrush x:Key="ZebraTableBrush2" Color="{DynamicResource ZebraTable2}" />
	
	<SolidColorBrush x:Key="IndicatorDataPresentBackgroundBrush" Color="#006400" />
	<SolidColorBrush x:Key="IndicatorNoDataBackgrounBrush" Color="#FFB6C1" />

	<SolidColorBrush x:Key="IndicatorDataPresentForegroundBrush" Color="#FFFFFF" />
	<SolidColorBrush x:Key="IndicatorNoDataForegrounBrush" Color="#000000" />


	<SolidColorBrush x:Key="DentalshareNotUploadYet" Color="LightGray" />
	<SolidColorBrush x:Key="DentalshareNotUploadYetBorder" Color="LightGray" />

	<SolidColorBrush x:Key="DentalshareQueuedForUpload" Color="Yellow" />
	<SolidColorBrush x:Key="DentalshareQueuedForUploadBorder" Color="Black" />

	<SolidColorBrush x:Key="DentalshareAlreadyUploaded" Color="LightGreen" />
	<SolidColorBrush x:Key="DentalshareAlreadyUploadedBorder" Color="Black" />


	<Color x:Key="MaterialLabelForeground">White</Color>
	<Color x:Key="MaterialLabelBackground">Black</Color>

	<SolidColorBrush x:Key="MaterialLabelForegroundBrush" Color="{DynamicResource MaterialLabelForeground}" />
	<SolidColorBrush x:Key="MaterialLabelBackgroundBrush" Color="{DynamicResource MaterialLabelBackground}" />

    <Color x:Key="ContentAndTextButtonDescriptionText">#FF7E7E7E</Color>
    <SolidColorBrush x:Key="ContentAndTextButtonDescriptionTextBrush" Color="{DynamicResource ContentAndTextButtonDescriptionText}"/>

    <!--Hover-->
    <LinearGradientBrush x:Key="HoverCheckBoxOuterBackground" EndPoint="1,1" StartPoint="0,0">
        <GradientStop Color="#969696" Offset="0"/>
        <GradientStop Color="#90ffffff" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="HoverCheckBoxOuterBorder" Color="#c98800"/>
    <SolidColorBrush x:Key="HoverCheckBoxInnerBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="HoverCheckBoxInnerBorder" Color="#90ffffff"/>
    <SolidColorBrush x:Key="HoverCheckBoxForeground" Color="{DynamicResource BrandColor2}"/>

    <!-- Treatment details window button colors -->
    <Color x:Key="TreatmentDetailsActionButtonNormalColor">Black</Color>
    <SolidColorBrush x:Key="TreatmentDetailsActionButtonNormalBrush" Color="{DynamicResource TreatmentDetailsActionButtonNormalColor}"/>
    <Color x:Key="TreatmentDetailsActionButtonClickedColor">White</Color>
    <SolidColorBrush x:Key="TreatmentDetailsActionButtonClickedBrush" Color="{DynamicResource TreatmentDetailsActionButtonClickedColor}"/>

    <!-- colors for buttons in expert options toolbar in load treatment window -->
    <Color x:Key="ExpertOptionsActionButtonNormalColor">White</Color>
    <SolidColorBrush x:Key="ExpertOptionsActionButtonNormalBrush" Color="{DynamicResource ExpertOptionsActionButtonNormalColor}"/>
    <Color x:Key="ExpertOptionsActionButtonClickedColor">Black</Color>
    <SolidColorBrush x:Key="ExpertOptionsActionButtonClickedBrush" Color="{DynamicResource ExpertOptionsActionButtonClickedColor}"/>
    <SolidColorBrush x:Key="TreatmentLoadExpertOptionsPanelBrush" Color="{DynamicResource BrandColor1}"/>



</ResourceDictionary>