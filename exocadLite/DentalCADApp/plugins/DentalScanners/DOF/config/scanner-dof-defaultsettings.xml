<!-- DO NOT MODIFY THIS FILE!
   
     It will be overwritten during update process
     
     Instead, copy it to "scanner-dof-settings.xml" and make your changes there
     (you can remove the tags that remain unchanged - entries in scanner-dof-settings.xml
     will override entries in scanner-dof-defaultsettings.xml) -->
     
<DentalScannerSettingsProviderDOF>
	<DentalScanner_DOF_CalibrationTimeOutDays>7</DentalScanner_DOF_CalibrationTimeOutDays>
	<DentalScanner_DOF_CutPointCloudsWithALowerPlane>false</DentalScanner_DOF_CutPointCloudsWithALowerPlane>
	<DentalScanner_DOF_CuttingPlaneDistance>20.0</DentalScanner_DOF_CuttingPlaneDistance>
	<DentalScanner_DOF_SupportImpressionScan>true</DentalScanner_DOF_SupportImpressionScan>
	<DentalScanner_DOF_ArticulatorUseTransferPlate>false</DentalScanner_DOF_ArticulatorUseTransferPlate>
	<DentalScanner_DOF_UseOpenMPWorkaround>true</DentalScanner_DOF_UseOpenMPWorkaround>
	<!-- Timeout for "projector brightness back to normal" (in seconds) -->
	<DentalScanner_DOF_BrightProjectorModeTimeout>180</DentalScanner_DOF_BrightProjectorModeTimeout>
	<!-- Using 'false' will keep projector running with normal brightness mode -->
	<DentalScanner_DOF_SwitchProjectorOffAfterTimeout>true</DentalScanner_DOF_SwitchProjectorOffAfterTimeout>
	<!-- Position for brightness adjustment -->
	<DentalScanner_DOF_BrightnessAdjustAxisA_Freedom>45</DentalScanner_DOF_BrightnessAdjustAxisA_Freedom>
	<DentalScanner_DOF_BrightnessAdjustAxisA_FreedomHD>45</DentalScanner_DOF_BrightnessAdjustAxisA_FreedomHD>
	<DentalScanner_DOF_BrightnessAdjustAxisA_Swing>-45</DentalScanner_DOF_BrightnessAdjustAxisA_Swing>
	<!-- Min/max ranges for brightness adjustment -->
	<DentalScanner_DOF_BrightnessRangeMin_Freedom>0</DentalScanner_DOF_BrightnessRangeMin_Freedom>
	<DentalScanner_DOF_BrightnessRangeMax_Freedom>100</DentalScanner_DOF_BrightnessRangeMax_Freedom>
	<DentalScanner_DOF_BrightnessRangeMin_FreedomHD>0.015</DentalScanner_DOF_BrightnessRangeMin_FreedomHD>
	<DentalScanner_DOF_BrightnessRangeMax_FreedomHD>50</DentalScanner_DOF_BrightnessRangeMax_FreedomHD>
	<DentalScanner_DOF_BrightnessRangeMin_Swing>0.015</DentalScanner_DOF_BrightnessRangeMin_Swing>
	<DentalScanner_DOF_BrightnessRangeMax_Swing>50</DentalScanner_DOF_BrightnessRangeMax_Swing>
</DentalScannerSettingsProviderDOF>
