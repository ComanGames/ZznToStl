<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:gui="clr-namespace:DentalGUIHelper;assembly=DentalConfig">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="../BaseControlStyles/ButtonStyle.xaml" />
        <ResourceDictionary Source="LinkButtonStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{x:Type gui:OnlineHelpButton}" BasedOn="{StaticResource BaseButtonStyle}" TargetType="{x:Type gui:OnlineHelpButton}"/>

    <Style x:Key="OnlineHelpButtonLink" BasedOn="{StaticResource LinkButton}" TargetType="{x:Type gui:OnlineHelpButton}"/>

</ResourceDictionary>