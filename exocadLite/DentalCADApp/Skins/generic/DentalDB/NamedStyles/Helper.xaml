<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:gui="clr-namespace:DentalGUIHelper;assembly=DentalConfig">
    <ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="..\..\Common\NamedStyles\NamedButtonStyles.xaml "/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="IronicallyNamedChromelessButtonStyle" TargetType="{x:Type Button}" BasedOn="{StaticResource IronicallyNamedChromelessButtonStyle}">
	<Style.Resources>
		<SolidColorBrush x:Key="BlackBrush" Color="{DynamicResource Gray5}"/>
	</Style.Resources>
        <Setter Property="Padding" Value="1" />
    </Style>
	
</ResourceDictionary>