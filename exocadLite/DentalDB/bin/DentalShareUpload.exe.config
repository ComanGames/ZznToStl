<?xml version="1.0"?>
<configuration>

  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="DentalShareUpload.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
      <section name="DentalShareService.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
      <section name="DentalShareCommunication.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
    <!-- Register a section handler for the log4net section -->
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler"/>
  </configSections>

  <runtime>
    <loadFromRemoteSources enabled="true"/>
    <generatePublisherEvidence enabled="false"/>
  </runtime>

  <system.net>
    <settings>
      <performanceCounters enabled="false"/>
    </settings>
  </system.net>

  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ExoshareImplPortBinding" closeTimeout="00:01:00"
                 openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
                 allowCookies="false" bypassProxyOnLocal="false" hostNameComparisonMode="StrongWildcard"
                 maxBufferSize="4194304" maxBufferPoolSize="8388608" maxReceivedMessageSize="4194304"
                 messageEncoding="Text" textEncoding="utf-8" transferMode="Buffered"
                 useDefaultWebProxy="false">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="4194304"
                        maxBytesPerRead="4096" maxNameTableCharCount="16384"/>
          <security mode="None">
            <transport clientCredentialType="None" proxyCredentialType="None" realm=""/>
            <message clientCredentialType="UserName" algorithmSuite="Default"/>
          </security>
        </binding>
      </basicHttpBinding>
      <netNamedPipeBinding>
        <binding name="FileTransferNetNamedPipeBinding" transferMode="Buffered"
                 maxBufferSize="4194304" maxBufferPoolSize="8388608" maxReceivedMessageSize="4194304" />
      </netNamedPipeBinding>
      <!--closeTimeout="00:01:00" openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"-->
      <!--hostNameComparisonMode="StrongWildcard" maxConnections ="10" transactionFlow="False" transactionProtocol="" />-->
    </bindings>

    <client>
      <endpoint address="http://localhost:8080/backend/ws/exoshare"
                binding="basicHttpBinding" bindingConfiguration="ExoshareImplPortBinding"
                contract="DentalShareWS.exoshare" name="ExoshareImplPort"
                behaviorConfiguration="WSencoding"/>
    </client>

    <services>
      <!--Hinweis: Der Dienstname muss dem Konfigurationsnamen f端r die Dienstimplementierung entsprechen.-->
      <service name="DentalShareService.FileTransfer" behaviorConfiguration="FileTransferSvcBehaviors">
        <!--<endpoint address="http://localhost:8000/FileTransfer/Transfer" binding="basicHttpBinding"
                  contract="DentalShareServiceInterface.IFileTransfer"/>-->
                  <!--behaviorConfiguration="SvcEncoding"/>-->
        <endpoint address="net.pipe://localhost/FileTransfer/Transfer" binding="netNamedPipeBinding"
                  bindingConfiguration="FileTransferNetNamedPipeBinding"
                  contract="DentalShareServiceInterface.IFileTransfer"/>
        <!--Hinweis: Der Dienst muss 端ber eine HTTP-Basisadresse verf端gen, um diesen Endpunkt hinzuzuf端gen.-->
        <!--<endpoint address="http://localhost:8000/FileTransfer/mex" contract="IMetadataExchange" binding="mexHttpBinding"/>-->
        <!--<host>
          <baseAddresses>
            <add baseAddress="http://localhost:8000/FileTransfer"/>
          </baseAddresses>
        </host>-->
      </service>
    </services>

    <extensions>
      <behaviorExtensions>
        <add name="ExoCryptStandardEndpointBehavior" type="DentalShareUtils.ExoCryptStandardEndpointBehavior, DentalShareUtils, Version=2.0.0.0, Culture=neutral, PublicKeyToken=6851684b7ebaad6c"/>
      </behaviorExtensions>
    </extensions>

    <behaviors>
      <serviceBehaviors>
        <behavior name="FileTransferSvcBehaviors">
          <serviceMetadata httpGetEnabled="false"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <!--<behavior name="SvcEncoding">-->
        <!--</behavior>-->
        <behavior name="WSencoding">
          <ExoCryptStandardEndpointBehavior/>
        </behavior>
      </endpointBehaviors>
    </behaviors>

  </system.serviceModel>

  <!-- This section contains the log4net configuration settings -->
  <log4net>
    <!-- Define some output appenders -->
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline"/>
      </layout>
    </appender>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="${APPDATA}\DentalDB\dentalshare\DentalShareUpload-log.txt"/>
      <appendToFile value="true"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="1020KB"/>
      <rollingStyle value="Size"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <!--<header value="[Header]&#13;&#10;" />-->
        <!--<footer value="[Footer]&#13;&#10;" />-->
        <!--<conversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline" />-->
        <conversionPattern value="%date [%thread] %-5level %logger %method(%line) - %message%newline"/>
      </layout>
    </appender>
    <!-- Setup the root category, add the appenders and set the default level -->
    <root>
      <level value="INFO" />
      <appender-ref ref="ConsoleAppender"/>
      <appender-ref ref="RollingLogFileAppender"/>
    </root>
  </log4net>

  <applicationSettings>
    <DentalShareUpload.Properties.Settings>
      <setting name="HideWindowOnClose" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowTrayToolTip" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowTrayToolTipVerbose" serializeAs="String">
        <value>False</value>
      </setting>
    </DentalShareUpload.Properties.Settings>
    
    <DentalShareService.Properties.Settings>
      <setting name="RetryWaitSeconds" serializeAs="String">
        <value>10</value>
      </setting>
      <setting name="RepeatErrorNotifySeconds" serializeAs="String">
        <value>3600</value>
      </setting>
      <setting name="SwitchToMirrorLevel" serializeAs="String">
        <value>3</value>
      </setting>
      <setting name="UploadChunkSize" serializeAs="String">
        <value>104857600</value>
      </setting>
      <setting name="UploadBlockSize" serializeAs="String">
        <value>32768</value>
      </setting>
      <setting name="ProgressUpdatesPerSecond" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="NoCompressFileExtensions" serializeAs="String">
        <value>dentalCAD,dentalCADWeb,transfermetadata,gz,jpeg,jpg,png,rar,zip,7z</value>
      </setting>
      <setting name="MinSkipLevel" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="MaxRetryLevel" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="LoadOpenJobsOfLastHours" serializeAs="String">
        <value>720</value>
      </setting>
      <setting name="LoadClosedJobsOfLastHours" serializeAs="String">
        <value>60</value>
      </setting>
      <setting name="AutoResumeOpenJobs" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="KeepCanceledJobs" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="KeepEncryptedFile" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="UseLocalUploadProxy" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="ServiceID" serializeAs="String">
        <value>FileTransfer</value>
      </setting>
    </DentalShareService.Properties.Settings>
  </applicationSettings>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
</configuration>
