<?xml version="1.0" encoding="utf-8"?>
<configuration>



  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="DentalShareConnector.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <!--For xml serialization debugging-->
  <!--
  <system.diagnostics>
    <switches>
      <add name="XmlSerialization.Compilation" value="4" />
    </switches>
  </system.diagnostics>
  -->

  <system.data>
    <DbProviderFactories>
      <remove invariant="System.Data.SQLite" />
      <add name="SQLite Data Provider" invariant="System.Data.SQLite" description=".Net Framework Data Provider for SQLite" type="System.Data.SQLite.SQLiteFactory, System.Data.SQLite" />

      <!--mysql-->
      <!--
 	 <remove invariant="MySql.Data"/>
	 <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" 
	 type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.5.4.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
	 -->
    </DbProviderFactories>
  </system.data>

  <!--
  <system.diagnostics>
    <sources>
      <source name="System.Windows.Data" switchName="SourceSwitch" >
        <listeners>
          <add name="textListener" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="SourceSwitch" value="All" />
    </switches>
    <sharedListeners>
      <add name="textListener" type="System.Diagnostics.ConsoleTraceListener" />
    </sharedListeners>
    <trace autoflush="true" indentsize="4"></trace>
  </system.diagnostics>
  -->

  <!-- This section contains the log4net configuration settings -->
  <log4net>

    <!-- Define some output appenders -->

    <appender name="rollingFile" type="log4net.Appender.RollingFileAppender,log4net">
      <param name="File" value="${APPDATA}\DentalDB\logs\dentaldb.log" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyyMMdd" />
      <maxSizeRollBackups value="7" />
      <param name="StaticLogFileName" value="true" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%date [%thread] %-5level %logger %class.%method(%line) - %message%newline" />
      </layout>
    </appender>

    <!-- Setup the root category, add the appenders and set the default priority -->

    <root>
      <priority value="ALL" />
      <level value="OFF" />
      <!--<appender-ref ref="rollingFile" />-->
    </root>

    <!-- add additivity="false" if it should not be added to root logger-->
    <logger name="ScanImport02" additivity="false">
      <level value="INFO" />
      <appender-ref ref="ScanImport02" />
    </logger>

    <appender type="log4net.Appender.RollingFileAppender,log4net" name="ScanImport02">
      <file value="${APPDATA}\DentalDB\logs\ScanImport02.log" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <maxSizeRollBackups value="7" />
      <param name="DatePattern" value="yyyyMMdd" />
      <param name="StaticLogFileName" value="true" />
      <threshold value="OFF" />
      <lockingModel type="DentalDB.DelayedLock" />
      <layout type="log4net.Layout.PatternLayout">
        <!--<conversionpattern value="%date [%thread] %-5level %logger %class.%method(%line) - %message%newline" />-->
        <conversionpattern value="%date - %message%newline" />
      </layout>
    </appender>

    <logger name="ScanImport04" additivity="false">
      <level value="INFO" />
      <appender-ref ref="ScanImport04" />
    </logger>

    <logger name="DentalProjectAutoImport">
      <level value="DEBUG" />
      <appender-ref ref="DentalProjectAutoImport" />
    </logger>

    <appender type="log4net.Appender.RollingFileAppender,log4net" name="DentalProjectAutoImport">
      <file value="${APPDATA}\DentalDB\logs\DentalProjectImport.log" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Date" />
      <maxSizeRollBackups value="7" />
      <param name="DatePattern" value="yyyyMMdd" />
      <param name="StaticLogFileName" value="true" />
      <!--<threshold value="DEBUG" />-->
      <lockingModel type="DentalDB.DelayedLock" />
      <layout type="log4net.Layout.PatternLayout">
        <!--<conversionpattern value="%date [%thread] %-5level %logger %class.%method(%line) - %message%newline" />-->
        <conversionpattern value="%date - %message%newline" />
      </layout>
    </appender>

  </log4net>

  <runtime>
    <generatePublisherEvidence enabled="false" />
    <loadFromRemoteSources enabled="true" />

  </runtime>

  <applicationSettings>
    <DentalShareConnector.Properties.Settings>
      <!-- custom configuration options for DentalShare-->
    </DentalShareConnector.Properties.Settings>
  </applicationSettings>
</configuration>